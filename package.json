  "dependencies": {
    "@ionic/cloud": "^0.8.2",
    "angular-moment": "^1.0.0",
    "angular-ui-router": "^0.3.1",
    "bluebird": "^3.4.6",
    "gulp": "^3.5.6",
    "gulp-concat": "^2.2.0",
    "gulp-minify-css": "^0.3.0",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "^2.0.4",
    "moment": "^2.17.0"
  },
  "devDependencies": {
    "bower": "^1.3.3",
    "gulp-util": "^2.2.14",
    "shelljs": "^0.3.0"
  },
  "cordovaPlugins": [
    {
